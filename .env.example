# NutriBuddy backend (server.js) - copy to .env and fill in
# Run backend: npm start  (uses PORT below; use 3002 so app can use 3000)

PORT=3002

# Supabase (required for saving food logs)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# One of these for nutrient lookup (Gemini is easier)
GEMINI_API_KEY=your-gemini-api-key
# USDA_API_KEY=your-usda-api-key
